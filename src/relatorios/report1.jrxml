	<queryString>
		<![CDATA[SELECT
     cheque.`id` AS cheque_id,
     cheque.`dataPagamento` AS cheque_dataPagamento,
     cheque.`dataVencimento` AS cheque_dataVencimento,
     cheque.`numero` AS cheque_numero,
