     cheque.`valor` AS cheque_valor,
     cheque.`idFuncionario` AS cheque_idFuncionario,
     cheque.`idMotivoDespesa` AS cheque_idMotivoDespesa,
     funcionario.`id` AS funcionario_id,
     funcionario.`cargo` AS funcionario_cargo,
     funcionario.`dataContratacao` AS funcionario_dataContratacao,
     funcionario.`matricula` AS funcionario_matricula,
     funcionario.`observacao` AS funcionario_observacao,
     funcionario.`salario` AS funcionario_salario,
     funcionario.`idPessoa` AS funcionario_idPessoa
FROM
     `funcionario` funcionario INNER JOIN `cheque` cheque ON funcionario.`id` = cheque.`idFuncionario`
order by cheque.id]]>
