<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="acal2000PU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>entidades.ContasMensais</class>
    <class>entidades.Contrato</class>
    <class>entidades.EntradasLog</class>
    <class>entidades.SaidasLog</class>
    <class>entidades.CategoriaSocio</class>
    <class>entidades.Funcionario</class>
    <class>entidades.Socio</class>
    <class>entidades.Pessoa</class>
    <class>entidades.Cheque</class>
    <class>entidades.Conta</class>
    <class>entidades.Endereco</class>
    <class>entidades.Entrada</class>
    <class>entidades.Taxa</class>
    <class>entidades.Saida</class>
    <class>entidades.MotivoEntrada</class>
    <class>entidades.MotivoDespesa</class>
    <class>entidades.Taxas</class>
    <class>entidades.Usuarios</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/acal?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.password" value="123"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
